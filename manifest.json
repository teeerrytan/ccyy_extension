{
    "manifest_version":2,
    "name": "Unkown-Vocabulary",
    "description":"Get the url",
    "version": "0.0.4",
    "options_page": "option.html",
    "permissions":[
        "http://*/*","https://*/*",
        "tabs",
        "cookies",
        "storage"
    ],
    "background":{
        "script":["js/popup.js","js/pdf.js"]},

    "content_scripts":[{
        "matches":["http://*/*","https://*/*","file:///*"],
        "js":["js/jquery-2.1.1.min.js","js/content.js"]

    }],
    "browser_action":{
        "default_icon":"asset/icon.png",
        "default_popup":"popup.html"
    }
}
